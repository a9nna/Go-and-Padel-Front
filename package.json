{
  "name": "go-and-padel",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:env": "ng generate environments && sed -i 's|};| production: '$PRODUCTION',\\n apiUrl: '$API_URL',\\n path: {\\n users: '$USERS_URL',\\n loginUser: '$LOGIN_URL',\\n matches: '$MATCHES_URL', \\n remove: '$DELETE_URL', \\n create: '$CREATE_URL' \\n }\\n};|' ./src/environments/environment.ts",
    "watch": "ng build --watch --configuration development",
    "test": "jest --no-cache",
    "test:dev": "jest --no-cache --watchAll",
    "test:coverage": "jest --no-cache --coverage",
    "prepare": "husky install",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/cdk": "^15.2.2",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@fontsource/manrope": "^4.5.13",
    "@ngrx/effects": "^15.3.0",
    "@ngrx/entity": "^15.3.0",
    "@ngrx/store": "^15.3.0",
    "@ngrx/store-devtools": "^15.3.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.6",
    "jest": "^29.5.0",
    "jest-preset-angular": "^13.0.0",
    "jwt-decode": "^3.1.2",
    "rxjs": "~7.8.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.1",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.2.1",
    "@angular/compiler-cli": "^15.2.0",
    "@ngrx/eslint-plugin": "^15.3.0",
    "@ngrx/schematics": "^15.3.0",
    "@testing-library/angular": "^13.3.0",
    "@testing-library/dom": "^9.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^0.56.0",
    "husky": "^8.0.0",
    "ng-lint-staged": "^12.0.4",
    "prettier": "2.8.4",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "ng-lint-staged lint --fix --"
    ]
  }
}
